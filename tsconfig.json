{
  "compilerOptions": {
    "target": "es2023",
    "module": "preserve",
    "types": [
      "node"
    ],
    "noEmit": true,
    "moduleResolution": "bundler",
    "strict": true,
    "noImplicitOverride": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "useUnknownInCatchVariables": false
  }
}
